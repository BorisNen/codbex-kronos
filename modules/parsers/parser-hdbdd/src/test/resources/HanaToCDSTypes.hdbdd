namespace sap.db;

@Schema : 'TEST_SCHEMA'

context HanaToCDSTypes {

    type MyType {
        field1 : UTCTimestamp;
        field2 : Integer;
    };

    @Catalog.tableType : #COLUMN
    entity MyTable {
     key ID    : Integer;
         COL_1 : String(20);
         COL_2 : hana.VARCHAR(10, 2);

         COL_4 : type of MyType.field1;
    }
};