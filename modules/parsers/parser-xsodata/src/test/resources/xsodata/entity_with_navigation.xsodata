service namespace "my.demo.namespace" {

//Navigation
    "sample.odata::customer" as "Customers"
		navigates ("Customer_Orders" as "HisOrders","Customer_Recruit" as "Recruit" from dependent);
	"sample.odata::student" as "Students"
        navigates ("Students_Grades" as "HisGrades" from principal);

    "sample.odata::Company" as "acme_vean__Company"
    	key ("UID")
    	navigates ("acme_vean__Company__Person" as "Person" from dependent);
    "sample.odata::Person" as "acme_bc_ar__Person"
    	key ("UID")
    	navigates ("acme_vean__Company__Person" as "acme_vean__Company" from principal);
    association "acme_vean__Company__Person"
    		principal "acme_bc_ar__Person"("UID") multiplicity "0..1"
    		dependent "acme_vean__Company"("Person.UID") multiplicity "0..1";

	"sample.odata::address" as "Address"
	 	navigates ("A_Self" as "ARef" from principal);
	association "A_Self"
		principal "Address"("SelfID") multiplicity "1"
		dependent "Address"("ID") multiplicity "0..1";
}